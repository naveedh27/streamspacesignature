  "source": "pragma solidity ^0.4.24;\n\ncontract Verifier{\n    \n    address owner;\n    \n    constructor() public{\n        owner = msg.sender;\n    }\n\n    function verify(bytes32 hash, uint8 v, bytes32 r, bytes32 s) constant public returns(bool) {\n        return ecrecover(hash, v, r, s) == owner;\n    }\n}",
